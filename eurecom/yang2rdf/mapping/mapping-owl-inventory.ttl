# ============================================================================

<NetworkInventoryClassMap> a rr:TriplesMap ;
  rml:logicalSource [ 
    rml:source "source.xml" ;
    rml:referenceFormulation ql:XPath ;
    rml:iterator "/yin:module/yin:namespace[@uri='urn:ietf:params:xml:ns:yang:ietf-network-inventory']"
  ] ;
  rr:subjectMap [ rr:constant ex:NetworkInventory ] ;
  rr:predicateObjectMap [
    rr:predicate rdf:type ;
    rr:objectMap [ rr:constant owl:Class ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate rdfs:label ;
    rr:objectMap [ rr:constant "Network Inventory"@en ]
  ] ;
  rr:predicateObjectMap [
    rr:predicate rdfs:comment ;
    rr:objectMap [ rr:constant "Top-level container for network inventory data model"@en ]
  ] .

