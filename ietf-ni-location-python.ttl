    rdfs:label "p2mp-port" ;
    rdfs:comment "Input port." ;
    rdfs:subClassOf <http://www.huawei.com/ontology/identity/passive-port-type> ;
    prov:wasDerivedFrom "nwi-passive:p2mp-port?identity" .

<http://www.huawei.com/ontology/identity/passive-device> a owl:Class,
        owl:NamedIndividual ;
    rdfs:label "passive-device" ;
    rdfs:comment "Passive/unmanaged device." ;
    rdfs:subClassOf <http://www.huawei.com/ontology/identity/connected-device-type> ;
    prov:wasDerivedFrom "nwi-passive:passive-device?identity" .

<http://www.huawei.com/ontology/identity/port> a owl:Class,
        owl:NamedIndividual ;
    rdfs:label "port" ;
    rdfs:comment """This identity is applicable if the hardware class is some sort
of networking port capable of receiving and/or transmitting
