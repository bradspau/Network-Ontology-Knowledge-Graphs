ex:ipv4-address a sh:NodeShape ;
    rdfs:label "ipv4-address" ;
    sh:datatype xsd:string ;
    sh:pattern "(([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])(%[\\p{N}\\p{L}]+)?" .

ex:ipv4-address-no-zone a sh:NodeShape ;
    rdfs:label "ipv4-address-no-zone" ;
    sh:datatype xsd:string ;
    sh:pattern "[0-9\\.]*" .

ex:ipv4-prefix a sh:NodeShape ;
    rdfs:label "ipv4-prefix" ;
    sh:datatype xsd:string ;
    sh:pattern "(([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9][0-9]|2[0-4][0-9]|25[0-5])/(([0-9])|([1-2][0-9])|(3[0-2]))" .

ex:ipv6-address a sh:NodeShape ;
    rdfs:label "ipv6-address" ;
    sh:datatype xsd:string ;
    sh:pattern "((:|[0-9a-fA-F]{0,4}):)([0-9a-fA-F]{0,4}:){0,5}((([0-9a-fA-F]{0,4}:)?(:|[0-9a-fA-F]{0,4}))|(((25[0-5]|2[0-4][0-9]|[01]?[0-9]?[0-9])\\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9]?[0-9])))(%[\\p{N}\\p{L}]+)?",
        "(([^:]+:){6}(([^:]+:[^:]+)|(.*\\..*)))|((([^:]+:)*[^:]+)?::(([^:]+:)*[^:]+)?)(%.+)?" .

ex:ipv6-address-no-zone a sh:NodeShape ;
    rdfs:label "ipv6-address-no-zone" ;
    sh:datatype xsd:string ;
    sh:pattern "[0-9a-fA-F:\\.]*" .

ex:ipv6-flow-label a sh:NodeShape ;
    rdfs:label "ipv6-flow-label" ;
    sh:datatype xsd:unsignedInt ;
    sh:maxInclusive 1048575 ;
    sh:minInclusive 0 .

ex:ipv6-prefix a sh:NodeShape ;
    rdfs:label "ipv6-prefix" ;
    sh:datatype xsd:string ;
    sh:pattern "((:|[0-9a-fA-F]{0,4}):)([0-9a-fA-F]{0,4}:){0,5}((([0-9a-fA-F]{0,4}:)?(:|[0-9a-fA-F]{0,4}))|(((25[0-5]|2[0-4][0-9]|[01]?[0-9]?[0-9])\\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9]?[0-9])))(/(([0-9])|([0-9]{2})|(1[0-1][0-9])|(12[0-8])))",
        "(([^:]+:){6}(([^:]+:[^:]+)|(.*\\..*)))|((([^:]+:)*[^:]+)?::(([^:]+:)*[^:]+)?)(/.+)" .

ex:link-id a sh:NodeShape ;
    rdfs:label "link-id" ;
    sh:datatype xsd:anyURI .

ex:mac-address a sh:NodeShape ;
    rdfs:label "mac-address" ;
    sh:datatype xsd:string ;
